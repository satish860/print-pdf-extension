{
  "name": "Print PDF Extension",
  "version": "0.1",
  "manifest_version": 3,
  "description": "Adds a Print PDF button to the chat.openai page.",
  "permissions": ["tabs", "activeTab"],
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["html2canvas.js", "jspdf.js", "html2pdf.js", "content.js"],
      "css": ["myscript.css"]
    }
  ],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
